<link rel="stylesheet" href="/css/pager.css">
<div class="pager-wrapper">
  <ul class="pager-wrap">
    <li class="pager pager-first">
      <a href="<%=user.id%>&page=1" class="material-icons">first_page</a>
    </li>
    <li class="pager pager-prev-set">
      <a href="<%=user.id%>&page=<%= user.id %>" class="material-icons">keyboard_double_arrow_left</a>
    </li>
    <li class="pager pager-prev">
      <a href="<%=user.id%>&page=<%= user.id %>" class="material-icons">chevron_left</a>
    </li>
    <% for(let i=1; i<=5; i++) { %>
    <li class="pager <%= user.id === i ? 'active' : '' %>">
      <a href="<%=user.id%>&page=<%= i %>"><%= i %></a>
    </li>
    <% } %>
    <li class="pager pager-next">
      <a href="<%=user.id%>&page=<%= user.id %>" class="material-icons">chevron_right</a>
    </li>
    <li class="pager pager-next-set">
      <a href="<%=user.id%>&page=<%= user.id %>" class="material-icons">keyboard_double_arrow_right</a>
    </li>
    <li class="pager pager-last">
      <a href="<%=user.id%>&page=<%= user.id %>" class="material-icons">last_page</a>
    </li>
  </ul>
</div>
